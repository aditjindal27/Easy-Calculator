<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="adit.easycalculator"
    android:versionCode="3"
    android:versionName="1.2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Converter"
            android:label="@string/title_activity_converter"
            android:theme="@style/aditstheme">

        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>



        <activity
            android:name=".Length"
            android:label="@string/title_activity_length"
            />
        <activity
            android:name=".Mass"
            android:label="@string/title_activity_mass"
          />
        <activity
            android:name=".Time"
            android:label="@string/title_activity_time"
            />
        <activity
            android:name=".Speed"
            android:label="@string/title_activity_speed"/>
        <activity
            android:name=".Energy"
            android:label="@string/title_activity_energy" />
        <activity
            android:name=".Volume"
            android:label="@string/title_activity_volume" />
        <activity
            android:name=".Temperature"
            android:label="@string/title_activity_temperature"
            />
        <activity
            android:name=".Area"
            android:label="@string/title_activity_area"/>
        <activity
            android:name=".easycalculator"
            android:label="easycalculator"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
